class Player{
  float x;
  float y;
  float d;
  Player() {
    x=0;
    y=0;
    d=100;
  }
  void display() {
    noStroke();
    fill(0);
    ellipse(x, y, d, d);
  }
void move() {
        if(BlackBox.isKeyDown(BlackBox.VK_NUMPAD4)){
        x-=5;
      }
      if(BlackBox.isKeyDown(BlackBox.VK_NUMPAD8)){
       y-=5;
      }
      if(BlackBox.isKeyDown(BlackBox.VK_NUMPAD5)){
      y+=5;
      }
      if(BlackBox.isKeyDown(BlackBox.VK_NUMPAD6)){
      x+=5;
      }
      
      
    }
  }

